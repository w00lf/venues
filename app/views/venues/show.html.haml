%p#notice= notice

%p
  %b Title:
  = @venue.title
%p
  %b Description:
  = @venue.description
%p
  %b Capasity:
  = @venue.capasity
%p
  %b Price:
  = @venue.price
%p
  %b Working time:
  = @venue.working_time
%p
  %b Address:
  = @venue.address
%p
  %b Metro:
  = @venue.metro
%p
  %b Event Types:
  - @venue.event_types.each do |type|
    = type.title

= link_to 'Edit', edit_venue_path(@venue)
\|
= link_to 'Back', venues_path

-if @venue.latitude && @venue.longitude
  %h3 map
  / %iframe{ width: "300", height: "170", frameborder: "0", scrolling: "no", marginheight: "0", marginwidth: "0", src: "https://maps.google.com/maps?q=#{@venue.latitude}%20#{@venue.longitude}&hl=es;z=14&amp;output=embed"}
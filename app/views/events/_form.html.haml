= form_for @event do |f|
  - if @event.errors.any?
    #error_explanation
      %h2= "#{pluralize(@event.errors.count, "error")} prohibited this event from being saved:"
      %ul
        - @event.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :venue_id
    = f.number_field :venue_id
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :desciption
    = f.text_area :desciption
  .actions
    = f.submit 'Save'
